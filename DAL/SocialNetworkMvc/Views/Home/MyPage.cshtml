@model SocialNetworkMvc.Models.MyPageModel
@{
    ViewBag.Title = "MyPage";
}

<div>
    <div class="container">
        <div class="row">
            <div class="col-sm">
                <img src="https://images.unsplash.com/photo-1518791841217-8f162f1e1131?ixlib=rb-1.2.1&auto=format&fit=crop&w=750&q=80" style="height:200px" />
            </div>
            <div class="col-sm">
                <h3> Basic info</h3>

                @Html.LabelFor(x => Model.Name)
                @Html.ValueFor(x => Model.Name)<br />

                @Html.LabelFor(x => Model.Email)
                @Html.ValueFor(x => Model.Email)<br />

                @Html.LabelFor(x => Model.City)
                @Html.ValueFor(x => Model.City)<br />

                @Html.LabelFor(x => Model.Age)
                @Html.ValueFor(x => Model.Age)<br />



            </div>

        </div>
    </div>
    <div class="container" style="margin-top:20px">
        <div class="row" >
            <div class="col-sm">

                @Html.Partial("_PartialViewPost")


            </div>
        </div>

    </div>
    </div>
